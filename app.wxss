/**app.wxss**/
button::after{
  border:none;
  }
button{
  margin-left:0;
  padding-left:0;
  margin-right:0;
  padding-right:0;
  text-align: left;
  border-radius:0rpx;
  background-color:rgba(0, 0, 0, 0);
  line-height: auto;
}
input{
  outline:none;
  border:none;
  list-style: none;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}



@font-face {
  font-family: 'iconfont';  /* project id 724078 */
  src: url('//at.alicdn.com/t/font_724078_59mf7bedyqa.eot');
  src: url('//at.alicdn.com/t/font_724078_59mf7bedyqa.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_724078_59mf7bedyqa.woff') format('woff'),
  url('//at.alicdn.com/t/font_724078_59mf7bedyqa.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_724078_59mf7bedyqa.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-bofang:before { content: "\e606"; }